<div>
    <script>

        async function fetchSpents() {

            var api = 'https://localhost:44338/api/Spent';
            const response = await fetch(api);
            return response.json();

        }

        async function makeElements() {
            var spents = await fetchSpents();
            spents.forEach((element) => $(".rockets").append(buildCard(element)));
        }

        makeElements();




    </script>
</div>